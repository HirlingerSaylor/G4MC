#!/usr/bin/perl

open INFILE, "<rtsep102008.map";
for my $i (0..5){
    my $input = <INFILE>;
    print "$input";
}
my @slurp = <INFILE>;
my @separ;
for ( @slurp ){
    my @numbers = ( $_ =~ /(\-?\d\.\d+(?:E(?:\+|\-|\W)?\d+)?)/g );
    #print "@numbers\n";
    for ( @numbers ){
	push( @separ, $_ );
    }
}

my $index = 1;
for ( @separ ) {
    print "$_ ";
    if ( $index % 3 == 0 && $index != 0 ) {
	print "\n";
    }
    $index++;
}
